
@{
    ViewBag.Title = "SignUp";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="log-reg container">
    <div class="row">
        <div class="col-lg-4 col-md-4 hidden-xs hidden-sm"></div>
        <div class="col-lg-4 col-md-4 col-xs-12 col-sm-12 formUser formSignUp">
            <h6>Đăng ký</h6>
            <form method="post" class="f" autocomplete="off">
                <ul>
                    <li>
                        <input name="fullName" type="text" class="tb validate[required]" id="fullName" placeholder="Họ và tên" value="">
                    </li>
                    <li>
                        <input name="email" type="text" class="tb validate[required,custom[email]]" id="email" placeholder="Email" value="">
                    </li>
                    <li>
                        <input name="password" type="password" class="tb validate[required,minSize[6]]" id="password" placeholder="Mật khẩu" value="">
                    </li>
                    <li>
                        <input name="mobile" type="text" class="tb validate[required,custom[phone]]" id="mobile" placeholder="Điện thoại" value="">
                    </li>
                    <li class="btns">
                        <input name="submit" type="submit" id="btnSubmit" class="htmlBtn first" value="Đăng ký">
                    </li>
                    <input type="hidden" name="csrf" value="b847854b838aa3dccfbe1394163b480b-2a6c4591066ec626bd1c75c4bd3f9854" />
                </ul>
            </form>
        </div>
        <div class="col-lg-4 col-md-4 hidden-xs hidden-sm">
        </div>
    </div>
</section>